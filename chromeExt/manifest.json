{
  "name": "Page Redder",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "storage",
    "http://*/",
    "http://192.241.182.93/*"
  ],
  "background": {
    "scripts": ["socket.io.js", "background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}